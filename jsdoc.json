{
  "source": {
    "include": ["./renderer.js", "./main.js"],
    "includePattern": ".+\\.js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash",
    "readme": "README.md"
  },
  "templates": {
    "default": {
      "includeDate": false,
      "collapseNav": true
    }
  }
}